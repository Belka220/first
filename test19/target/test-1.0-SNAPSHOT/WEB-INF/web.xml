<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>

    <display-name>ContactFilterTest</display-name>

    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>com.becomejavasenior.controller.Upload</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

    <filter>
        <display-name>ContactFilter</display-name>
        <filter-name>ContactFilter</filter-name>
        <filter-class>com.becomejavasenior.controller.ContactFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>ContactFilter</filter-name>
        <url-pattern>/ContactConfirm.jsp</url-pattern>
    </filter-mapping>
